<%# locals used: locals.settings.title, locals.titlePostfix, locals.lang, locals.css, locals.user %>
<%- include('includes/head.ejs', {title:locals.settings.title, titlePostfix:locals.titlePostfix, lang:locals.lang}); %>
 <body>
  <table border='1px'>
   <tr><td style='padding:0 0.5em;text-align:right;'><strong>id:</strong></td><td style='padding:0 0.5em;text-align:left;'><code><%= locals.user.id %></code></tr>
   <tr><td style='padding:0 0.5em;text-align:right;'><strong>username:</strong></td><td style='padding:0 0.5em;text-align:left;'><code><%= locals.user.username %></code></tr>
   <tr><td style='padding:0 0.5em;text-align:right;'><strong>displayName:</strong></td><td style='padding:0 0.5em;text-align:left;'><code><%= locals.user.displayName %></code></tr>
   <tr><td style='padding:0 0.5em;text-align:right;'><strong>emails:</strong></td><td style='padding:0 0.5em;text-align:left;'>
   <% if (locals.user.emails) { %>
   <%   locals.user.emails.forEach((el,idx)=>{ %>
    <strong><%= (idx+1) %>.</strong><code>&nbsp;</code><code><a href='mailto:<%= el.value %>'><%= el.value %></a></code><br>
   <%   }) %>
   <% } else { %>
     <code>None</code>
   <% }%>
   </td></tr>
  </table>
  <div><br><hr><a href='/logout'>Log out</a></div>
 </body>
</html>




